<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFrameworks>netstandard2.0;netstandard2.1;net6.0</TargetFrameworks>
		<Authors>Daniel Chandler</Authors>
		<Company>Proximity Interactive</Company>
		<Copyright>Copyright Â© Daniel Chandler 2020</Copyright>
		<Product>Proximity Collections Utility Library</Product>
		<Version>1.4.0</Version>
		<AssemblyVersion>1.4.0.0</AssemblyVersion>
		<FileVersion>1.4.0.0</FileVersion>
		<Description>Collections Utility Library</Description>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<GenerateDocumentationFile>true</GenerateDocumentationFile>
		<SignAssembly>true</SignAssembly>
		<AssemblyOriginatorKeyFile>..\Proximity.snk</AssemblyOriginatorKeyFile>
		<OutputPath>..\..\Build</OutputPath>
		<LangVersion>Latest</LangVersion>
		<PackageProjectUrl>https://github.com/AlphaGremlin/Proximity.Libraries</PackageProjectUrl>
		<RepositoryUrl></RepositoryUrl>
		<AllowedOutputExtensionsInPackageBuildOutputFolder>$(AllowedOutputExtensionsInPackageBuildOutputFolder);.pdb</AllowedOutputExtensionsInPackageBuildOutputFolder>
		<Nullable>enable</Nullable>
	</PropertyGroup>
	<ItemGroup Condition="$(TargetFramework)=='netstandard2.0'">
		<PackageReference Include="Microsoft.Bcl.AsyncInterfaces" Version="1.1.0" />
	</ItemGroup>
	<ItemGroup>
	  <Compile Remove="Immutable\ImmutableSortedList.cs" />
	  <Compile Remove="Immutable\ImmutableSortedList_2.Builder.cs" />
	  <Compile Remove="Immutable\ImmutableSortedList_2.Collections.cs" />
	  <Compile Remove="Immutable\ImmutableSortedList_2.cs" />
	  <Compile Remove="Immutable\ImmutableSortedList_2.Enumerator.cs" />
	  <Compile Remove="Immutable\ImmutableSortedList_2.KeyComparer.cs" />
	  <Compile Remove="Immutable\ImmutableSortedList_2.Node.cs" />
	  <Compile Remove="Observable\ObservableSortedSubset.cs" />
	</ItemGroup>
	<ItemGroup Condition="$(TargetFramework)!='net4.0'">
		<PackageReference Include="System.Memory" Version="4.5.4" />
		<PackageReference Include="System.Collections.Immutable" Version="5.0.0" />
	</ItemGroup>
	<ItemGroup>
	  <None Include="Immutable\ImmutableSortedList.cs" />
	  <None Include="Immutable\ImmutableSortedList_2.Builder.cs" />
	  <None Include="Immutable\ImmutableSortedList_2.Collections.cs" />
	  <None Include="Immutable\ImmutableSortedList_2.cs" />
	  <None Include="Immutable\ImmutableSortedList_2.Enumerator.cs" />
	  <None Include="Immutable\ImmutableSortedList_2.KeyComparer.cs" />
	  <None Include="Immutable\ImmutableSortedList_2.Node.cs" />
	  <None Include="Observable\ObservableSortedSubset.cs" />
	</ItemGroup>
</Project>
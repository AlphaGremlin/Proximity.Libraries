<xsl:stylesheet version="1.0" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns="http://www.w3.org/TR/xhtml1/strict">	<xsl:template match="/">		<html>			<head>				<title>Log File - Started <xsl:value-of select="@StartTime"/></title>				<style type="text/css">					.Section { padding-left: 50px; }					.SectionTitle { margin: 0px; padding: 0px; font-size: medium; }					.Error { color: red; }					.Warning { color: yellow; }					.Milestone { color: teal; }					.Info { color: grey; }					.Verbose { color: lightgrey; }				</style>			</head>			<body>				<xsl:apply-templates />			</body>		</html>	</xsl:template>	<xsl:template name="Section" match="Section">		<xsl:element name="h{count(ancestor::Section)+1}">			<xsl:attribute name="class">SectionTitle <xsl:value-of select="@Severity" /></xsl:attribute>			<xsl:value-of select="Title/text()" />		</xsl:element>		<div class="Section">			<xsl:apply-templates select="Section | Entry"/>		</div>	</xsl:template>	<xsl:template name="Entry" match="Entry">		<div>			<xsl:attribute name="class">				<xsl:value-of select="@Severity" />			</xsl:attribute>			<xsl:value-of select="text()" />		</div>	</xsl:template></xsl:stylesheet>
<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFrameworks>netstandard2.0</TargetFrameworks>
		<Authors>Daniel Chandler</Authors>
		<Company>Proximity Interactive</Company>
		<Copyright>Copyright © Daniel Chandler 2018</Copyright>
		<Product>Proximity Utility Library</Product>
		<AssemblyVersion>19.0.0.0</AssemblyVersion>
		<FileVersion>19.1.0.0</FileVersion>
		<Version>19.1.0</Version>
		<Description>Proximity Utility Library</Description>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<GenerateDocumentationFile>true</GenerateDocumentationFile>
		<SignAssembly>true</SignAssembly>
		<AssemblyOriginatorKeyFile>..\Proximity.snk</AssemblyOriginatorKeyFile>
		<OutputPath>..\..\Build\Proximity.Utility</OutputPath>
		<LangVersion>Latest</LangVersion>
	</PropertyGroup>
	<PropertyGroup Condition="'$(Configuration)'=='Debug'">
		<DebugSymbols>true</DebugSymbols>
		<DebugType>full</DebugType>
	</PropertyGroup>
	<ItemGroup>
		<Compile Remove="Collections\ObservableSortedSubset.cs" />
		<Compile Remove="Diagnostics\ExceptionExtensions.cs" />
		<Compile Remove="Logging\Outputs\XmlFileOutput.cs" />
		<Compile Remove="Logging\Outputs\XmlFileOutputElement.cs" />
	</ItemGroup>
	<ItemGroup>
		<None Include="Collections\ObservableSortedSubset.cs" />
		<None Include="Diagnostics\ExceptionExtensions.cs" />
		<None Include="Logging\Outputs\XmlFileOutput.cs" />
		<None Include="Logging\Outputs\XmlFileOutputElement.cs" />
	</ItemGroup>
	<ItemGroup>
		<PackageReference Include="System.Configuration.ConfigurationManager" Version="4.4.1" />
		<PackageReference Include="System.Memory" Version="4.5.1" />
		<PackageReference Include="System.Reflection.Emit.Lightweight" Version="4.3.0" />
		<PackageReference Include="System.Collections.Immutable" Version="1.4.0" />
	</ItemGroup>
</Project>